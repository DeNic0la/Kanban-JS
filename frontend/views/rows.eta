<div class="p-2">
    <div class="row">
        <% it.cols.forEach(function(col){%>
        <div class="col">
            <div class="card bg-light">
                <div class="card-body">
                    <h6 class="card-title text-uppercase text-truncate py-2"><%= col.name %></h6>

                </div>
            </div>
        </div>
        <% }) %>
    </div>
</div>
